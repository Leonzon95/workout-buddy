<div class="form__padding">
    <div class="container">
        <div class="row">

            <div class="col-md-6">
                <h1>My Workouts</h1>

                <%= link_to 'Create a new workout plan', new_workout_path %>
                <p>
                <%= render partial: 'workout_name_link', locals: {workouts: @workouts} %>
                </p>
                
            </div>

            <div class="col-md-6">
                <div class="img__down">
                    <%= image_tag 'working_out.svg' %>
                </div>
            </div>

        </div>
    </div>
</div>
