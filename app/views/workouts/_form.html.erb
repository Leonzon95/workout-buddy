<%= form_for workout do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :category %>
    <%= f.collection_select :category_id, Category.all, :id, :name, :prompt => true %>
    <%= f.radio_button :is_private, true %>
    <%= f.label :is_private %>
    <%= f.submit %>
<% end %>